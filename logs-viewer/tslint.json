{
  "extends": "tslint:recommended",
  "rules": {
    "interface-name": [false],
    "arrow-parens": false,
    "no-string-literal": false,
    "max-line-length": [false],
    "object-literal-sort-keys": false,
    "ordered-imports": [false],
    "no-console": [false]
  },
  "jsRules": {
    "curly": false,
    "object-literal-sort-keys": false,
    "one-variable-per-declaration": [false],
    "variable-name": [false],
    "max-line-length": [200]
  }
}
